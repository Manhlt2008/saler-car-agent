# Database Configuration (SQLite for development)
DATABASE_URL=sqlite:///car_agent.db

# Flask Configuration
SECRET_KEY=your-secret-key-here
FLASK_ENV=development

# API Keys - Replace with your actual keys
# Get OpenAI API key from: https://platform.openai.com/
OPENAI_API_KEY=your-openai-api-key-here

# Get Google Maps API key from: https://console.cloud.google.com/
# Enable: Places API, Maps JavaScript API
GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here

# Get SendGrid API key from: https://sendgrid.com/
SENDGRID_API_KEY=your-sendgrid-api-key-here
SENDGRID_FROM_EMAIL=noreply@caragent.com

# Frontend Configuration
REACT_APP_API_URL=http://localhost:5000
